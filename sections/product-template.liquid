{% comment %} CAPTURE HOW TO STEPS {% endcomment %}
{% capture howToStep %}
  {% for step in product.metafields.accentuate.how_to_steps %} 
    <div class="fd-product__howTo-step">
      <p class="number">{{ forloop.index }}</p>
      <div class="line-vertical"></div>
      <p class="info">{{ step }}</p>
    </div>
  {% endfor %} 
{% endcapture %}


<section class="fd-product__howTo">
  <div class="fd-content-block-product fd-container-width fd-product__howTo-inner">
    <div class="fd-product__howTo-info">
      <div class="fd-product__howTo-icons">
       {{ svgIcons }}
      </div>
        <h3>Directions for use</h3>
        <p class="posology">{{ product.metafields.accentuate.when_to_use }}</p>

        <div class="fd-product__howTo-steps">
         
          {{ howToStep }}

        </div>

        <p class="footer-info">{{ product.metafields.accentuate.footer_information }}</p>

      
    </div>


    <div class="fd-product__howTo-video">
      {% if product.metafields.foundry.video_url %}
        <div class="video-frame" id="videoFrame" style="
            position: relative;
            padding-bottom: 56.29%;" > 
        </div>
      {% elsif product.metafields.foundry.media %}
      {% assign howToImage = product.metafields.foundry.media | first %}
      <figure>
        <img src="{{ howToImage.src }}" alt="{{ howToImage.alt | escape }}">
      </figure> 

      {% else %}

      <figure>
        <img src="{{ 'video-placeholder.jpeg' | asset_img_url: 'master'}}" alt="product image">
      </figure> 
      {% endif %}

 
    </div>
  </div> 
</section>

<div class="fd-product__reviews fd-content-block-product fd-container-width">
  {% comment %}Start automatically added Judge.me widget{% endcomment %}
    {% render 'judgeme_widgets', widget_type: 'judgeme_review_widget', concierge_install: true, product: product %}
  {% comment %}End automatically added Judge.me widget{% endcomment %}
</div>


<script>

  var HowToUrl = {{ product.metafields.foundry.video_url.value | json }}
  var frame = document.getElementById('videoFrame')
  function convert_youtube(input) {
    var pattern = /(?:http?s?:\/\/)?(?:www\.)?(?:youtube\.com|youtu\.be)\/(?:watch\?v=)?(\S+)/g;
    if (pattern.test(input)) {
      var replacement = '<iframe style="pointer-events:none; position:absolute; top:0; left:0; width:100%; height:100%; " width="100%" height="100%" src="http://www.youtube.com/embed/$1?autoplay=1&mute=1&enablejsapi=1&loop=1&playlist=$1&controls=0" frameborder="0" ></iframe>';
      var input = input.replace(pattern, replacement);
  
    }
    return input;
  } 

   //function convert_vimeo(input) {
   //  var pattern = /(?:http?s?:\/\/)?(?:www\.)?(?:vimeo\.com)\/?(\S+)/g;
   //  if (pattern.test(input)) {
   //  var replacement = '<iframe width="420" height="345" src="//player.vimeo.com/video/$1" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>';
   //  var input = input.replace(pattern, replacement);
   //  }
   //  return input;
   //}

  const embed = convert_youtube(HowToUrl)

  frame.innerHTML = embed

</script>


